# Google AI API Key for Gemini
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Feature flag para habilitar/desabilitar integração com Gemini
AI_GEMINI_ENABLED=true

# Modelo do Gemini a ser utilizado (gemini-2.5-flash é mais rápido e eficiente)
# Sugestão 2026: Gemini 3 Flash (preview) para melhor velocidade/leitura de docs
# (Se preferir estabilidade/GA, use: gemini-2.5-flash)
AI_MODEL_NAME=gemini-3-flash-preview

# Timeout em milissegundos para chamadas ao Gemini
AI_TIMEOUT_MS=60000

# Número máximo de tokens de saída
AI_MAX_OUTPUT_TOKENS=8192

# Habilitar cache de respostas (reduz chamadas ao Gemini)
AI_CACHE_ENABLED=true

# Tenta uma segunda passada (modelo mais forte) se detectar capacidades truncadas
# por quebra de página/linha no PDF.
AI_REPAIR_TRUNCATION_ENABLED=true
